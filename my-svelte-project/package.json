{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "tailwind:watch": "postcss src/tailwind.css -o public/index.css -w",
    "tailwind:build": "cross-env NODE_ENV=production postcss public/tailwind.css -o public/index.css",
    "build": "npm run tailwind:build && rollup -c",
    "dev": "concurrently \"rollup -c -w\" \"npm run tailwind:watch\"",
    "start": "sirv public --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "serve": "serve public -p 80"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "autoprefixer": "^10.3.7",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "postcss-cli": "^9.0.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "tailwindcss": "^2.2.17",
    "typescript": "^4.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "tslib": "^2.0.0",
    "@tsconfig/svelte": "^2.0.0"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0",
    "svelte-icons": "^2.1.0",
    "svelte-navigator": "^3.1.5"
  }
}
